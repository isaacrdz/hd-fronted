@font-face {
	font-family: 'icomoon';
	src:url('../fonts/icomoon.eot?-evbsp3');
	src:url('../fonts/icomoon.eot?#iefix-evbsp3') format('embedded-opentype');
	src:url('../fonts/icomoon.woff?-evbsp3') format('woff');
	src:url('../fonts/icomoon.ttf?-evbsp3') format('truetype');
	src:url('../fonts/icomoon.svg?-evbsp3#icomoon') format('svg');
	font-weight: normal;
	font-style: normal;
}

[class^="icon-"], [class*=" icon-"] {
	font-family: 'icomoon';
	speak: none;
	font-style: normal;
	font-weight: normal;
	font-variant: normal;
	text-transform: none;
	line-height: 1;

	/* Better Font Rendering =========== */
	-webkit-font-smoothing: antialiased;
	-moz-osx-font-smoothing: grayscale;
}

.icon-quill:before {
	content: "\e500";
}
.icon-camera:before {
	content: "\e501";
}
.icon-envelope:before {
	content: "\e502";
}
.icon-location:before {
	content: "\e503";
}
.icon-drawer:before {
	content: "\e504";
}
.icon-cog:before {
	content: "\e505";
}
.icon-star:before {
	content: "\e506";
}
.icon-pin:before {
	content: "\e507";
}
